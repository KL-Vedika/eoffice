{
  "manifest_version": 3,
  "name": "Form Filler from Document",
  "version": "1.2",
  "description": "Gets Document ID and schema from current page, data from API, and fills the page's form. API endpoint is configurable.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  }
}